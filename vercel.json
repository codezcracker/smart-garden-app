{
  "version": 2,
  "name": "smart-garden-iot",
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "regions": ["iad1"],
  "framework": "nextjs"
}
  